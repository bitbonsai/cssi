{
  "name": "cssi",
  "description": "CSS Selector Scene Investigation. Analyses a git codebase for unused CSS selectors against a given CSS files set",
  "version": "0.5.0",
  "homepage": "https://github.com/bitbonsai/cssi",
  "author": {
    "name": "Mauricio Wolff",
    "email": "eu@mauriciowolff.com",
    "url": "http://mauriciowolff.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bitbonsai/cssi.git"
  },
  "bugs": {
    "url": "https://github.com/bitbonsai/cssi/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/bitbonsai/cssi/blob/master/LICENSE-MIT"
    }
  ],
  "main": "./lib/cssi.js",
  "engines": {
    "node": ">= 0.8.0"
  },
  "scripts": {
    "test": "grunt nodeunit",
    "lint": "jshint --verbose bin/ lib/ test/"
  },
  "jshintConfig": {
    "node": true,
    "strict": true,
    "validthis": true
  },
  "devDependencies": {
    "grunt-contrib-jshint": "~0.6.4",
    "grunt-contrib-nodeunit": "~0.2.0",
    "grunt-contrib-watch": "~0.5.3",
    "grunt": "~0.4.5"
  },
  "preferGlobal": "true",
  "bin": {
    "cssi": "./bin/cssi.js"
  },
  "keywords": [
    "css",
    "analyzer",
    "complexity",
    "webperf"
  ],
  "dependencies": {
    "colors": "^0.6.2",
    "css-parse": "^2.0.0",
    "event-emitter": "^0.3.1",
    "exec-queue": "0.0.2",
    "minimist": "^1.1.0"
  }
}
